# Include ~/.orbstack/ssh/config

Host *
    # ForwardX11Trusted yes
    # ForwardX11 yes
    ForwardAgent no
    UseKeychain yes
    AddKeysToAgent yes
    # IdentityFile ~/.ssh/id_rsa
    IdentityFile ~/.ssh/id_ed25519
    ServerAliveInterval 20
    ServerAliveCountMax 10
    # XAuthLocation /opt/X11/bin/xauth
    SetEnv TERM=xterm-256color
