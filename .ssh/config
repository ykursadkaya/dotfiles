# Include ~/.orbstack/ssh/config

Host *
    # ForwardX11Trusted yes
    # ForwardX11 yes
    ForwardAgent yes
    UseKeychain yes
    AddKeysToAgent yes
    IdentityFile ~/.ssh/id_rsa
    IdentityFile ~/.ssh/id_ed25519
    ServerAliveInterval 60
    ServerAliveCountMax 5
    XAuthLocation /opt/X11/bin/xauth
